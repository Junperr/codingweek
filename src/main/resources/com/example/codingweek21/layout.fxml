<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox prefHeight="36.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.codingweek21.controller.LayoutController">

    <children>
      <ToolBar prefHeight="36.0" prefWidth="607.0">
        <items>
            <Label fx:id="myProfile" onMouseClicked="#goToMyProfilePage" text="My profile" />
            <Label fx:id="offers" onMouseClicked="#goToOffers" text="Offers" />
            <Label fx:id="logout" alignment="CENTER_RIGHT" contentDisplay="RIGHT" onMouseClicked="#logout" text="Logout" />
        </items>
      </ToolBar>
   </children>
</HBox>
