<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="50.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.codingweek.controller.MiniConvController">
   <children>
      <HBox layoutY="-1.0">
         <children>
            <VBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="250.0" onMouseClicked="#goToConv">
               <children>
                  <Label fx:id="otherName" prefHeight="18.0" prefWidth="128.0" />
                  <Label fx:id="lastMessage" prefHeight="18.0" prefWidth="243.0" />
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </VBox>
            <VBox alignment="CENTER" prefHeight="50.0" prefWidth="50.0">
               <children>
                  <Label fx:id="time" prefHeight="18.0" prefWidth="34.0" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
